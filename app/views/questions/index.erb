<%= render "users/nav"%>

<div class="grid-container question">
  <% @questions.each do |question| %>
      <div class="grid-item question"><%=link_to question.title, question_path(@current_user, question) %><br/>
        <% question.categories.uniq.each do |category|%>
       <span class="tag"><%=link_to category.name, category_path(category)%></span>
          <%end%>
      <%end%>
    </div>
</div>
