<%= form_for(:session, url: login_path) do |f|%>
<%=f.label "Email Address"%>
<%=f.email_field :email, placeholder: "Enter email" %>
<%=f.label :password%>
<%=f.password_field :password, placeholder: "Password"%>
<%=f.submit "Log in"%>
<%end%>
<p">New user?<%=link_to "Sign up now!", new_user_path%></p>
